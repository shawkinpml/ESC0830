{
	// 1. 生成elf*3 2. 生成gen.S*3 3.再次生成elf*3
    "tasks": [
        /////////////////   arm rootboot  //////////////////////
        {
            "type": "shell",
            "label": "mkdir_arm_rootboot",
            "command": "mkdir",
            "args": ["-Force",
                    "./ARM/rootboot/build"]
        },
        {
            "type": "shell",
            "label": "rm_build_arm_rootboot",
            "command": "Remove-Item",
            "args": ["-Path",
                     "./ARM/rootboot/build",
                     "-Recurse"],
            "dependsOn": [
                "mkdir_arm_rootboot"
            ]
        },
        {
            "type": "shell", 
            "label": "mk_build_arm_rootboot",
            "command": "mkdir",
            "args": ["./ARM/rootboot/build"],
            "dependsOn": [
                "rm_build_arm_rootboot"
            ]
        },
        {
            "type": "shell",
            "label": "cmake_arm_rootboot",
            "command": "cmake",
            "args": [
                "-DCMAKE_VERBOSE_MAKEFILE=ON",
                "-G",
                "MinGW Makefiles",
                "${workspaceFolder}/ARM/rootboot"
            ],
            "options": {
                "cwd": "${workspaceFolder}/ARM/rootboot/build",
            },
            "dependsOn": [
                "mk_build_arm_rootboot"
            ]
        },
        {
            "type": "shell",
            "label": "make_arm_rootboot",
            "command": "mingw32-make",
            "args": ["-j8",
                     "V=1"],
            // "args": ["V=1"],
            "options": {
                "cwd": "${workspaceFolder}/ARM/rootboot/build",
            },
            "dependsOn": [
                "cmake_arm_rootboot"
            ]
        },
        {
            "type": "shell",
            "label": "build_arm_rootboot",
            "command": "cp",
            "args": ["./ARM/rootboot/build/arm_rootboot.*",
                     "../output"],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "options": {
                "cwd": "${workspaceFolder}",
            },
            "dependsOrder": "sequence",
            "dependsOn": [
                "make_arm_rootboot" // 这里设置，说明了这个任务一定要在Task “cmake”执行之后方可执行
            ]
        },
